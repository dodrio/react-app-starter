{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "polished": "^1.9.0",
    "promise.prototype.finally": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "styled-components": "^3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "prettier": "^1.10.2",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-mobx": "^1.0.7",
    "react-app-rewire-polished": "^1.0.3",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-app-rewired": "^1.4.0",
    "source-map-explorer": "^1.5.0",
    "stylelint": "^8.4.0",
    "stylelint-config-prettier": "^2.0.0",
    "stylelint-config-standard": "^18.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "precommit": "lint-staged",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "src/**/*.{js,jsx,json,css}": ["prettier --write"],
      "src/**/*.{js,jsx,json}": ["eslint"],
      "src/**/*.css": ["stylelint"],
      "src/**/*.{js,jsx,json,css}": ["git add"]
    }
  }
}
